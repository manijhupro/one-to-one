<article id="contact" class="section-wrapper clearfix" data-custom-background-img="<%= image_path('bg4.jpg')%>">
    <div class="content-wrapper clearfix">

        <h1 class="section-title">Contact</h1>

        <!-- CONTACT DETAILS -->
        <div class="contact-details col-sm-5 col-md-3">
            <p>123A,<br/>Molestie Odio Avenue,<br/>Aliquam<br/>AAA0010</p>
            <p>Tel: (+70) 81 451 576</p>
            <p><%= link_to 'info@loremipsum.com', 'mailto:info@loremipsum.com'%></p>
        </div>
        <!-- END: CONTACT DETAILS -->

        <!-- CONTACT FORM -->
        <div class="col-sm-7 col-md-9">

            <%= form_for('', :html=>{:class => 'form-style validate-form clearfix', :role=>'form'}) do |f| %>

                <!-- form left col -->
                <div class="col-md-6">
                    <div class="form-group">
                        <%= f.text_field 'name', class:'text-field form-control validate-field required', id:'form-name', data:{:'validation-type'=>'string'}, placeholder:'Full Name'%>
                    </div>
                    <div class="form-group">
                        <%= f.text_field 'email', type:'email', class:'text-field form-control validate-field required', id:'form-email', data:{:'validation-type'=>'email'}, placeholder:'Email Address'%>
                    </div>
                    <div class="form-group">
                        <%= f.text_field 'contact_number', type:'tel', class:'text-field form-control validate-field phone', id:'form-contact-number', data:{:'validation-type'=>'phone'}, placeholder:'Contact Number'%>
                    </div>
                </div><!-- end: form left col -->

                <!-- form right col -->
                <div class="col-md-6">
                    <div class="form-group">
                        <%= f.text_area 'message', class:'form-control validate-field required', placeholder:'Message...'%>
                    </div>
                    <div class="form-group">
                        <%=image_tag('loader-form.GIF', class:"form-loader") %>
                            <%= f.submit 'Submit', class:'btn btn-sm btn-outline-inverse' %>
                    </div>
                    <div class="form-group form-general-error-container"></div>
                </div><!-- end: form right col -->

            <% end %>
        </div><!-- end: CONTACT FORM -->

    </div><!-- .content-wrapper -->
</article><!-- .section-wrapper -->

