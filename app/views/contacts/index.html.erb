<h1>Listing contacts</h1></br>
<%= link_to button_to 'Back', root_path %>


<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Title</th>
    <th>Body</th>
    <th>Phnumber</th>
      <th>Date</th>

      <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.title %></td>
    <td><%= contact.body %></td>
    <td><%= contact.phnumber %></td>
      <td>

          <!--%= Time.now.strftime("%Y-%h-%d %I:%M:%S %p") %>-->
          <%= contact.created_at.strftime("%Y-%h-%d %I:%M:%S %p")%>

      </td>
    <td><%= link_to 'Show', contact %></td>
    <!--<td>%= link_to 'Edit', edit_contact_path(contact) %></td>-->
    <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

